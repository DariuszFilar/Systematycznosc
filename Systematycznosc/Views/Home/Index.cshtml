<header class="masthead bg-primary text-white text-center">
    <div class=" d-flex align-items-center flex-column">

        <!-- Masthead Avatar Image -->
        <img src="~/Content/Images/darek.jpg" class="img-circle" style="max-width:305px; padding-top:85px;">

        <!-- Masthead Heading -->
        @if (Model.UserProfileViewModel.Name != null)
        {
            <h1 class="masthead-heading text-uppercase mb-0" style="font-size:55px; padding-top:55px; line-height: 50px;">Witaj @Model.UserProfileViewModel.Name</h1>
        }

        <!-- Icon Divider -->
        <div class="divider-custom divider-light" style="padding-top:20px;">
            <div class="divider-custom-line"></div>
            <div class="divider-custom-icon">
                <i class="glyphicon glyphicon-star" style="font-size: 55px;"></i>
            </div>
            <div class="divider-custom-line"></div>
        </div>

        <!-- Masthead Subheading -->
        <p class="masthead-subheading font-weight-light mb-0">W pomocniku systematyczności</p>

    </div>
    @{
        if (Model.CredoViewModel.Credo1 == null && Model.EveningQuestionsViewModel.EveningQuestion1 == null && Model.FamilyBirthdayViewModel.FamilyBirthday1 == null && Model.FriendsBirthdayViewModel.FriendsBirthday1 == null
